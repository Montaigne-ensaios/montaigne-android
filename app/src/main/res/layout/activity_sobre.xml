<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.Sobre">

    <TextView
        android:id="@+id/textSobreConteudo"
        android:layout_width="332dp"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="70dp"
        android:fontFamily="serif"
        android:text="string sobre_conteudo"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textSobreNois"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgBntVoltarTelaInicial"
        app:layout_constraintTop_toBottomOf="@+id/textSobreNois" />

    <TextView
        android:id="@+id/textSobreConteudo2"
        android:layout_width="322dp"
        android:layout_height="115dp"
        android:layout_marginTop="20dp"
        android:fontFamily="serif"
        android:text="string app_contato"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textSobreConteudo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textSobreConteudo"
        app:layout_constraintTop_toBottomOf="@+id/textSobreConteudo" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar8"
        android:layout_width="403dp"
        android:layout_height="79dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textSobreNois"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="string sobre"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/toolbar8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgBntVoltarTelaInicial"
        android:layout_width="43dp"
        android:layout_height="38dp"
        android:layout_marginEnd="20dp"
        android:background="@color/transparente"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/textSobreNois"
        app:layout_constraintEnd_toStartOf="@+id/textSobreNois"
        app:layout_constraintTop_toTopOf="@+id/textSobreNois"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/btn_dialog"
        app:tint="@color/white" />
</androidx.constraintlayout.widget.ConstraintLayout>